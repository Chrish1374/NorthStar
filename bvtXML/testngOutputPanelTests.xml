<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="Viewer Page Tests" verbose="1"> <!--parallel="classes" thread-count="2"> -->


	<listeners>
		<listener class-name="org.uncommons.reportng.HTMLReporter" />
		<listener
			class-name="org.uncommons.reportng.JUnitXMLReporter" />
	</listeners>

	<test name="Output Panel cine" preserve-order="true">
		<groups>
			<run>
				<include name="BVT" />
			</run>
		</groups>
		<classes>
			<class
				name="com.trn.ns.test.viewer.outputPanel.CineInOutputPanelTest" />
		</classes>
	</test>

	<test name="Output Panel Expand Collapse" preserve-order="true">
		<groups>
			<run>
				<include name="BVT" />
			</run>
		</groups>
		<classes>
			<class
				name="com.trn.ns.test.viewer.outputPanel.ExpandCollapseFunctTest" />
		</classes>
	</test>

