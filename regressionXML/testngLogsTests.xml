<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="Logs testing" verbose="1"> <!--parallel="classes" thread-count="2"> -->
	<listeners>
		<listener class-name="org.uncommons.reportng.HTMLReporter" />
		<listener
			class-name="org.uncommons.reportng.JUnitXMLReporter" />
	</listeners>

	<test name="User Action logs tests" preserve-order="true">
		<classes>
			<class name="com.trn.ns.test.logs.UserActionLogsTest" />
		</classes>
	</test>
	
</suite>
